			EndIf
		Next nJ

		dbCommit()
		MsUnLock()

		AutoGrLog( "Campo creado " + aSX3[nI][nPosCpo] )

	EndIf

	oProcess:IncRegua2( "Actualizando Campos de Tablas (SX3) ..." )

Next nI

